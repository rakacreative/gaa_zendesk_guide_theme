<section class="section hero">
    <div class="hero-overlay"></div>
    <div class="hero__inner layout-container container-fluid container-sm">
        <h1 class="hero__heading">Welcome to GAA Connect</h1>
        <p class="hero__intro">This space is where seafood professionals can <strong>connect</strong> with each other,
            hear about the latest research, and ask questions. Just getting started? Use search to find answers, or
            enter the community to browse topics and posts.</p>
        <h2 class="visibility-hidden">{{ t 'search' }}</h2>
        {{search submit=false instant=settings.instant_search class='search search-full'}}
    </div>
</section>

